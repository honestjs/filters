{
	"name": "@honestjs/zod-exception-filter",
	"version": "0.0.0",
	"description": "Zod exception filter for HonestJS framework",
	"main": "./dist/index.js",
	"module": "./dist/index.mjs",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
