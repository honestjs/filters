	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist",
		"type-check": "tsc --noEmit"
	},
	"keywords": [
		"honestjs",
		"hono",
		"nestjs",
		"filter"
	],
	"author": "Orkhan Karimov <karimovok1@gmail.com> (https://github.com/kerimovok)",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/honestjs/filters.git",
		"directory": "packages/zod-exception-filter"
	},
	"dependencies": {},
	"devDependencies": {
		"hono": "^4.8.5",
		"honestjs": "^0.1.4",
		"zod": "^4.0.5"
	},
	"peerDependencies": {
		"hono": "^4.8.4",
		"honestjs": "^0.1.4",
		"zod": "^4.0.0"
	}
}
